<%= simple_form_for(@honor_society) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :mission %>
    <%= f.input :gpa %>
    <%= f.input :orgsync %>
    <%= f.input :infourl %>
    <%= f.input :applyurl %>
    <%= f.input :major %>
  </div>

  

<div class="field">
  <%= f.label :tag_list, "Tags (separated by commas)" %><br />
  <%= f.text_field :tag_list %>
</div>



  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
